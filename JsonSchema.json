{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "T1D-UOM â€“ A Longitudinal Multimodal Dataset of Type 1 Diabetes",
  "type": "object",
  "properties": {
    "UoMActivityID": {
      "type": "object",
      "properties": {
        "activity_ts": { "type": "string", "format": "date-time" },
        "activity_type": { "type": "string" },
        "active_Kcal": { "type": "integer" },
        "step_count": { "type": "integer" },
        "distance_m": { "type": "number" },
        "duration_s": { "type": "integer" },
        "active_time_s": { "type": "integer" },
        "start_time_s": { "type": "integer" },
        "start_time_offset_s": { "type": "integer" },
        "met": { "type": "number" },
        "intensity": { "type": "string" },
        "motion_intensity_mean": { "type": "number" },
        "motion_intensity_max": { "type": "number" }
      },
      "required": [
        "activity_ts",
        "activity_type",
        "active_Kcal",
        "step_count",
        "distance_m",
        "duration_s",
        "active_time_s",
        "start_time_s",
        "start_time_offset_s",
        "met",
        "intensity",
        "motion_intensity_mean",
        "motion_intensity_max"
      ]
    },
    "UoMBasalID": {
      "type": "object",
      "properties": {
        "basal_ts": { "type": "string", "format": "date-time" },
        "basal_dose": { "type": "number" },
        "insulin_kind": { "type": "string" }
      },
      "required": ["basal_ts", "basal_dose", "insulin_kind"]
    },
    "UoMBolusID": {
      "type": "object",
      "properties": {
        "bolus_ts": { "type": "string", "format": "date-time" },
        "bolus_dose": { "type": "number" }
      },
      "required": ["bolus_ts", "bolus_dose"]
    },
    "UoMGlucoseID": {
      "type": "object",
      "properties": {
        "bg_ts": { "type": "string", "format": "date-time" },
        "value": { "type": "number" }
      },
      "required": ["bg_ts", "value"]
    },
    "UoMNutritionID": {
      "type": "object",
      "properties": {
        "meal_ts": { "type": "string", "format": "date-time" },
        "meal_type": { "type": "string" },
        "meal_tag": { "type": "string" },
        "carbs_g": { "type": "integer" },
        "prot_g": { "type": "integer" },
        "fat_g": { "type": "integer" },
        "fibre_g": { "type": "integer" }
      },
      "required": [
        "meal_ts",
        "meal_type",
        "meal_tag",
        "carbs_g",
        "prot_g",
        "fat_g",
        "fibre_g"
      ]
    },
    "UoMsleepID": {
      "type": "object",
      "properties": {
        "sleep_ts": { "type": "string", "format": "date-time" },
        "step_count": { "type": "integer" },
        "heart_rate": { "type": "integer" },
        "current_activity_type_intensity": { "type": "integer" },
        "stress_level_value": { "type": "integer" },
        "sleep_level": { "type": "integer" },
        "resting_heart_rate": { "type": "integer" }
      },
      "required": [
        "sleep_ts",
        "step_count",
        "heart_rate",
        "current_activity_type_intensity",
        "stress_level_value",
        "sleep_level",
        "resting_heart_rate"
      ]
    },
    "UoMIDsleeptime": {
      "type": "object",
      "properties": {
        "sleep_start_ts": { "type": "string", "format": "date-time" },
        "sleep_end_ts": { "type": "string", "format": "date-time" },
        "calendar_date": { "type": "string", "format": "date-time" },
        "sleep_window_confirm_type": { "type": "string" },
        "deep_sleep_s": { "type": "integer" },
        "light_sleep_s": { "type": "integer" },
        "rem_sleep_s": { "type": "integer" },
        "awake_sleep_s": { "type": "integer" },
        "unmeasurable_s": { "type": "integer" },
        "sleep_levels_map_deep": { "type": "string" },
        "sleep_levels_map_light": { "type": "string" },
        "sleep_levels_map_awake": { "type": "string" },
        "sleep_levels_map_rem": { "type": "string" },
        "sleep_levels_map_unmeasurable": { "type": "string" },
        "validation": { "type": "string" }
      },
      "required": [
        "sleep_start_ts",
        "sleep_end_ts",
        "calendar_date",
        "sleep_window_confirm_type",
        "deep_sleep_s",
        "light_sleep_s",
        "rem_sleep_s",
        "awake_sleep_s",
        "unmeasurable_s",
        "sleep_levels_map_deep",
        "sleep_levels_map_light",
        "sleep_levels_map_awake",
        "sleep_levels_map_rem",
        "sleep_levels_map_unmeasurable",
        "validation"
      ]
    }
  }
}
